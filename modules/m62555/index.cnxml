   <m:mrow>
    <m:mi>z</m:mi><m:mo>&lt;</m:mo><m:mn>1.36</m:mn></m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>0.9131</m:mn></m:mrow>
