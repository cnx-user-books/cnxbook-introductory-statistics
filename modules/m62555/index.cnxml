    <m:mi>z</m:mi><m:mo>&lt;</m:mo><m:mn>1.47</m:mn></m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>−</m:mo><m:mn>0.9292</m:mn><m:mo>=</m:mo><m:mn>0.0708</m:mn></m:mrow>
</m:math>
