  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>−</m:mo><m:mi>P</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mi>z</m:mi><m:mo>&lt;</m:mo><m:mo>−</m:mo><m:mn>0.33</m:mn></m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>−</m:mo><m:mn>0.3707</m:mn><m:mo>=</m:mo><m:mn>0.6293</m:mn></m:mrow>
</m:math>
